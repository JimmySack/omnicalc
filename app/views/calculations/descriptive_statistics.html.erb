<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Descriptive Statistics Results</h3>
  </div>
  <div class="panel-body">
    <dl class="dl-horizontal">
      <dt>
        Numbers
      </dt>
      <dd id="numbers">
        <%= @numbers %>
      </dd>

      <dt>
        Sorted Numbers
      </dt>
      <dd id="sorted_numbers">
        <%= @sorted_numbers %>
      </dd>

      <dt>
        Count
      </dt>
      <dd id="count">
        <%= @count %>
      </dd>

      <dt>
        Minimum
      </dt>
      <dd id="minimum">
        <%= @minimum %>
      </dd>

      <dt>
        Maximum
      </dt>
      <dd id="maximum">
        <%= @maximum %>
      </dd>

      <dt>
        Range
      </dt>
      <dd id="range">
        <%= @range %>
      </dd>

      <dt>
        Median
      </dt>
      <dd id="median">
        <%= @median %>
      </dd>

      <dt>
        Sum
      </dt>
      <dd id="sum">
        <%= @sum %>
      </dd>

      <dt>
        Mean
      </dt>
      <dd id="mean">
        <%= @mean %>
      </dd>

      <dt>
        Variance
      </dt>
      <dd id="variance">
        <%= @variance %>
      </dd>

      <dt>
        Standard Deviation
      </dt>
      <dd id="standard_deviation">
        <%= @standard_deviation %>
      </dd>

      <dt>
        Mode
      </dt>
      <dd id="mode">
        <%= @mode %>
      </dd>
    </dl>
  </div>
</div>
